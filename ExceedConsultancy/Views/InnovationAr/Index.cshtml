@model ExceedConsultancy.Models.BusinessModel
@{
    Layout = "~/Views/Shared/_LayoutAr.cshtml";

    ViewBag.Title = "Exceed Consultancy | Business Consultancy Services";
    ViewBag.MetaDescription = "Our business consultancy services are tailored to address your unique challenges and opportunities. Whether you need to develop a robust business strategy, streamline processes for maximum efficiency, or gain deeper insights into your target market, we have you covered.";
    ViewBag.MetaKeywords = "Business Consultancy, Strategy Development, Business Planning, Acquisitions, Process Optimization, Financial Analysis, Market Analysis";

}



<body>


    <div id="anywhere-home">
    </div>
    <!-- ENd Header Area -->
    <!-- start breadcrumb area -->
    <div class="rts-breadcrumb-area breadcrumb-bg bg_image">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 breadcrumb-1">
                    <h1 class="title">    استراتيجية الابتكار والتسليم</h1>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                    <div class="bread-tag">
                        <a asp-controller="Home" asp-action="index">الرئيسية</a>
                        <span> / </span>
                        استراتيجية الابتكار والتسليم
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end breadcrumb area -->
    <!-- start service details area -->
    <div class="rts-service-details-area rts-section-gap">
        <div class="container">
            <div class="row">
                <div class="col-xl-8 col-md-12 col-sm-12 col-12">
                    <!-- service details left area start -->
                    <div class="service-detials-step-1">
                        <div class="thumbnail">
                            <img src="assets/images/Innovation-Strategy-.jpg" alt="business-area">
                        </div>
                        <h4 class="title">    استراتيجية الابتكار والتسليم</h4>
                        <p class="disc">
                            الابتكار يدفع النمو المستدام. نحن نساعد الشركات في تطوير استراتيجيات الابتكار وتقديم مبادراتها المبتكرة. تعزز حلولنا ثقافة الإبداع وريادة الأعمال ، مما يضمن بقاء مؤسستك في طليعة الابتكار في مجال عملك.


                            في تتجاوز للاستشارات ، ونحن حريصون على مساعدة الشركات على تحقيق إمكاناتهم الكاملة عبر هذه المجالات الهامة. اتصل بنا اليوم لاستكشاف كيف يمكن لخدماتنا الاستشارية أن تدفع عملك إلى الأمام.
                        </p>

                    </div>
                    <!-- service details left area end -->

                </div>
                <!--rts blog wizered area -->
                <div class="col-xl-4 col-md-12 col-sm-12 col-12 mt_lg--60 pl--50 pl_md--0 pl-lg-controler pl_sm--0">
                    <!-- single wizered start -->
                    <div class="rts-single-wized Categories service">
                        <div class="wized-header">
                            <h5 class="title">
                                Categories
                            </h5>
                        </div>
                        <div class="wized-body">
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="BusinessAr" asp-action="index">القطاع التجاري  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <!-- single categoris End -->
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="FinanceAr" asp-action="index">تمويل الشركات واستراتيجيتها <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <!-- single categoris End -->
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="TechnologyAr" asp-action="index">قطاع التكنولوجيا<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ArtificialIntelligenceAr" asp-action="index">الذكاء الاصطناعي<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="MonitoringEvaluationAr" asp-action="index">مراقبة وتطوير الشركات  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ManufacturingAr" asp-action="index">الصناعة<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="MarketingAr" asp-action="index">النمو والتسويق والمبيعات <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="OperationsAr" asp-action="index">قطاع العمليات <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ImplementationAr" asp-action="index">التخطيط والتنفيذ<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="OrganizationStrategyAr" asp-action="index">وضع الإسترايجيات<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="PeopleStrategyAr" asp-action="index">الموارد البشرية  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="PricingAr" asp-action="index">وضع الأسعار وإدارة الموارد  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="BudgetingAr" asp-action="index">تنظيم الميزانية والمصاريف<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="RiskManagementAr" asp-action="index">إدارة المخاطر وتجاوزها  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="InnovationAr" asp-action="index">استراتيجية الابتكار والتسليم  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>

                            @*   <br />*@
                            @*<img src="assets/images/service/services.jpg" />*@
                            <!-- single categoris End -->
                        </div>
                    </div>

                </div>
            </div>

        </div>
    </div>

</body>