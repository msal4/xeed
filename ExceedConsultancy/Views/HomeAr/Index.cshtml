@model ExceedConsultancy.Models.HomeModel
@{
    Layout = "~/Views/Shared/_LayoutAr.cshtml";
    ViewBag.Title = "Xeed Consulting | الخبرة الاستراتيجية";
    ViewBag.MetaDescription = "مرحبًا بكم في اكسيد للاستشارات! نحن فريق مكرس من المستشارين الخبراء يعزز الأعمال بالاستراتيجيات المبتكرة والحلول المدفوعة بالتكنولوجيا. شارك معنا للتعامل مع التحديات المعقدة، واستغلال فرص النمو، وتحقيق النجاح المستدام.";
    ViewBag.MetaKeywords = "اكسيد للاستشارات، تمكين الأعمال، الابتكار، الخبرة الاستراتيجية، الحلول المبتكرة، التحديات المعقدة، فرص النمو، النجاح المستدام، استشارات الأعمال، تطوير الاستراتيجية، التخطيط الاستراتيجي، الاستحواذات، تحسين العمليات، تحليل مالي، تحليل السوق";

