
@model ExceedConsultancy.Models.HomeModel;
@using Microsoft.AspNetCore.Html;



@using System.Globalization;
@using Microsoft.AspNetCore.Localization;

@{
    var culture = ViewContext.HttpContext.Features.Get<IRequestCultureFeature>().RequestCulture.Culture.Name;
    var textDirection = culture.StartsWith("ar") ? "rtl" : "ltr";
}

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = @SharedResource["title_home"];
    ViewBag.MetaDescription = @SharedResource["MetaDescription_home"];
    ViewBag.MetaKeywords = @SharedResource["MetaKeywords_home"];
}
    <div id="anywhere-home"></div>

<div>
    @*@Html.Partial("_CreateSliderForm", Model.Slider)*@
</div>
    <div class="rts-banner-area-two">
        <div class="swiper mySwiperh2_banner">
            <div class="swiper-wrapper">
            @if (Model.Slider != null)
            {
                @foreach (var sliderItem in Model.Slider.OrderBy(item => item.Order))
                {
                    <div class="swiper-slide">
                        <div class="banner-two" style="background-image: url('../assets/images/@sliderItem.Image');">
                            <div class="container">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="banner-two-content text-center">
                                            <div class="wrapper">
                                                <h1 class="title">
                                                    @if (CultureInfo.CurrentCulture.Name.StartsWith("ar"))
                                                    {
                                                        <span>@sliderItem.Title_AR</span>
                                                    }
                                                    else
                                                    {
                                                        <span>@sliderItem.Title_EN</span>
                                                    }
                                                </h1>


                                                <a class="rts-btn btn-primary-2" asp-controller="Home" asp-action="Services" >@SharedResource["Read_More"]</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
            else
            {
                <p>No data available.</p>
            }
            </div>
        </div>
    </div>
    <!-- rts banner area end -->
    <!-- business case start -->
    <div class="rts-business-case rts-section-gap business-case-bg-2 ">
        <div class="container">
            <div class="row">
                <div class="title-area text-center business-case">
                    <!--<span>Case Studies</span>-->
                    <h2 class="title">@SharedResource["Industries_Served"]	</h2>
                </div>
            </div>
        </div>
        <div class="container-cusiness-case-h2 mt--50">
            <div class="row">
                <div class="col-12">
                <div class="swiper mySwiperh2_Business_Cases">
                  
                            <div class="swiper-wrapper Industries">
                              @if (Model.Industrie != null)
                                {
                                    @foreach (var IndustrieItem in Model.Industrie.OrderBy(item => item.Order))
                                    {
                                <div class="swiper-slide">
                                    <div class="rts-business-case-s-2">

                                        <img src="../../assets/images/@IndustrieItem.Image" alt="@IndustrieItem.text">
                                        <div class="inner">
                                            <h5 class="title">
                                                @if (CultureInfo.CurrentCulture.Name.StartsWith("ar"))
                                                {
                                                    <span>@IndustrieItem.text_Ar</span>
                                                }
                                                else
                                                {
                                                    <span>@IndustrieItem.text</span>
                                                }
                                            </h5>
                                        </div>

                            


                                    </div>
                                </div>
                            }
                        }
                        else
                        {
                            <p>No data available.</p>
                        }
                            </div>
                    
                </div>
                </div>
            </div>
        </div>
    </div>
   
    <div class="rts-cta-section-start rts-section-gap cta-bg-h2">
        <div class="container">
            <div class="row">
                <div class="cta-h2-wrapper text-center">
                    <div class="icon">
                        <a href="#"><i class="fas fa-phone-alt"></i></a>
                    </div>
                    <div class="body">
                        <p class="info">@SharedResource["Home_bg_text_Contact"] <span>@*(24/7 Available)*@</span></p>
                        <a asp-controller="Quote" asp-action="index"  class="rts-btn btn-primary-2">@SharedResource["nav_Contact"]</a>
                    </div>
                </div>
            </div>
        </div>


    </div>
    <div class="rts-business-case-area rts-section-gapTop business-case-bg">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="title-area-business-case-3">
                        <div class="title-area">
                            <h2 class="title">@SharedResource["nav_capabilities"]</h2>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container-2 mt--65">
              
                <div class="row g-5">

                    <div class="swiper mySwiperh3_business-case">

                        <div class="swiper-wrapper">
                                 @if (Model.Capabilitie != null)
                                {
                                    @foreach (var CapabilitieItem in Model.Capabilitie.OrderBy(item => item.Order))
                                    {

                            <div class="swiper-slide">
                                <!-- single case study -->
                                <div class="rts-cse-study-3-wrapper">
                                    <div class="thumbnail">
                           
                                        <img src="assets/images/@CapabilitieItem.Image" alt="@CapabilitieItem.text">
                                        <a asp-controller="Home" asp-action="Business" asp-route-id="@CapabilitieItem.Id" class="icon">
                                            <i class="far fa-eye"></i>
                                        </a>
                                        <div class="content">
                                            <div class="content-wrap">
                                                <a asp-controller="Home" asp-action="Business" asp-route-id="@CapabilitieItem.Id">

                                                    @if (CultureInfo.CurrentCulture.Name.StartsWith("ar"))
                                                    {
                                                        <span>@CapabilitieItem.text_Ar</span>
                                                    }
                                                    else
                                                    {
                                                        <span>@CapabilitieItem.text</span>
                                                    }
                                                </a>
                                            </div>
                                        </div>
                                        <a asp-controller="Home" asp-action="Business" asp-route-id="@CapabilitieItem.Id" class="over_link"></a>
                               
                                    </div>
                                </div>
                            </div>
                        }
                    }
                    else
                    {
                        <p>No data available.</p>
                    }

                        </div>

                    </div>

                </div>
           
                
        </div>
    </div>
