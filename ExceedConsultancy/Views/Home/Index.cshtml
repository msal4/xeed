        <div class="container">
        @if (Model.WhyUsHome != null)
        {

            @foreach (var WhyUsitem in Model.WhyUsHome)
            {


            <div class="row g-5 align-items-center">
                <div class="col-lg-6">
                    <div class="image-area">
                            <img src="../assets/images/@WhyUsitem.Image1" alt="Service_Image">
                            <img class="two" src="../assets/images/@WhyUsitem.Image2" alt="Service_Image">
                            <img class="three" src="../assets/images/@WhyUsitem.Image4" alt="Service_Image">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="service-h2-content pl--30">
                        <div class="title-area  service-h2 service">
                            <span>@SharedResource["Why_Choose_Us"]</span>


                        <h2 class="title">
                                    @if (CultureInfo.CurrentCulture.Name.StartsWith("ar"))
                                    {
                                        @WhyUsitem.Title_Ar
                                    }
                                    else
                                    {
                                        @WhyUsitem.Title
                                    }
                        </h2>
                        </div>
                        <div class="content-wrapper">
                            <p class="disc">
                                    @if (CultureInfo.CurrentCulture.Name.StartsWith("ar"))
                                    {
                                        @WhyUsitem.Description_Ar
                                    }
                                    else
                                    {
                                           @WhyUsitem.Description
                                    }
                            </p>
                            <div class="feature-one-wrapper mt--40">
                                <div class="single-feature-one">
                                    <i class="fal fa-check"></i>
                                    <p>@SharedResource["Growing_Sells"]</p>
                                </div>
                                <div class="single-feature-one">
                                    <i class="fal fa-check"></i>
                                    <p>@SharedResource["Quality_Services"]</p>
                                </div>
                                <div class="single-feature-one">
                                    <i class="fal fa-check"></i>
                                <p>@SharedResource["Expert_Members"]</p>
                                </div>
                                <div class="single-feature-one">
                                    <i class="fal fa-check"></i>
                                <p>@SharedResource["Best_Quality_Services"]</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            }
        }
        else{

            <p>No Data Avialable</p>
         }
        </div>
    </div>
