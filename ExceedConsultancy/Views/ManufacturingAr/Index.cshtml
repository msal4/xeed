@model ExceedConsultancy.Models.BusinessModel
@{
    Layout = "~/Views/Shared/_LayoutAr.cshtml";

    ViewBag.Title = "Exceed Consultancy | Business Consultancy Services";
    ViewBag.MetaDescription = "Our business consultancy services are tailored to address your unique challenges and opportunities. Whether you need to develop a robust business strategy, streamline processes for maximum efficiency, or gain deeper insights into your target market, we have you covered.";
    ViewBag.MetaKeywords = "Business Consultancy, Strategy Development, Business Planning, Acquisitions, Process Optimization, Financial Analysis, Market Analysis";

}



<body>


    <div id="anywhere-home">
    </div>
    <!-- ENd Header Area -->
    <!-- start breadcrumb area -->
    <div class="rts-breadcrumb-area breadcrumb-bg bg_image">
        <div class="container">
            <div class="row align-items-center">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12 breadcrumb-1">
                    <h1 class="title">التصنيع</h1>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                    <div class="bread-tag">
                        <a asp-controller="Home" asp-action="index">الرئيسية</a>
                        <span> / </span>
                        التصنيع
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end breadcrumb area -->
    <!-- start service details area -->
    <div class="rts-service-details-area rts-section-gap">
        <div class="container">
            <div class="row">
                <div class="col-xl-8 col-md-12 col-sm-12 col-12">
                    <!-- service details left area start -->
                    <div class="service-detials-step-1">
                        <div class="thumbnail">
                            <img src="assets/images/Manufacturing-.jpg" alt="business-area">
                        </div>
                        <h4 class="title">التصنيع</h4>
                        <p class="disc">
                            تحقيق التميز التشغيلي في التصنيع من خلال خدماتنا الاستشارية. نحن نساعد الشركات على تبسيط عمليات التصنيع الخاصة بها ، وتعزيز مراقبة الجودة ، وتقليل النفايات. تضمن خبرتنا أن تكون عمليات التصنيع الخاصة بك فعالة وفعالة من حيث التكلفة وقادرة على تلبية أعلى معايير الصناعة.
                        </p>

                    </div>
                    <!-- service details left area end -->

                </div>
                <!--rts blog wizered area -->
                <div class="col-xl-4 col-md-12 col-sm-12 col-12 mt_lg--60 pl--50 pl_md--0 pl-lg-controler pl_sm--0">
                    <!-- single wizered start -->
                    <div class="rts-single-wized Categories service">
                        <div class="wized-header">
                            <h5 class="title">
                                Categories
                            </h5>
                        </div>
                        <div class="wized-body">
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="BusinessAr" asp-action="index">الغرض التجاري والتنظيمي  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <!-- single categoris End -->
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="FinanceAr" asp-action="index">تمويل الشركات واستراتيجيتها <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <!-- single categoris End -->
                            <!-- single categoris -->
                            <ul class="single-categories">
                                <li><a asp-controller="TechnologyAr" asp-action="index">الرقمية والتكنولوجيا والبيانات<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ArtificialIntelligenceAr" asp-action="index">الذكاء الاصطناعي<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="MonitoringEvaluationAr" asp-action="index">الرصد والتقييم  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ManufacturingAr" asp-action="index">التصنيع<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="MarketingAr" asp-action="index">النمو والتسويق والمبيعات <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="OperationsAr" asp-action="index">العمليات <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="ImplementationAr" asp-action="index">التنفيذ<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="OrganizationStrategyAr" asp-action="index">استراتيجية المنظمة<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="PeopleStrategyAr" asp-action="index">استراتيجية الناس  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="PricingAr" asp-action="index">التسعير وإدارة الإيرادات  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="BudgetingAr" asp-action="index">الميزنية الصفرية<i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="RiskManagementAr" asp-action="index">إدارة المخاطر والامتثال  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>
                            <ul class="single-categories">
                                <li><a asp-controller="InnovationAr" asp-action="index">استراتيجية الابتكار والتسليم  <i class="far fa-long-arrow-right"></i></a></li>
                            </ul>

                            @*   <br />*@
                            @*<img src="assets/images/service/services.jpg" />*@
                            <!-- single categoris End -->
                        </div>
                    </div>

                </div>
            </div>

        </div>
    </div>

</body>