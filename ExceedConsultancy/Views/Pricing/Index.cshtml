@model ExceedConsultancy.Models.BusinessModel
@using Microsoft.AspNetCore.Localization;

@{
    var culture = ViewContext.HttpContext.Features.Get<IRequestCultureFeature>().RequestCulture.Culture.Name;
    var textDirection = culture.StartsWith("ar") ? "rtl" : "ltr";
}
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
                                              

    ViewBag.Title = "Xeed Consulting | nav_pricings";
    ViewBag.MetaDescription = "nav_pricing  - Maximize your profitability with our nav_pricing  expertise. We help businesses optimize pricing strategies, revenue models, and demand forecasting. Our solutions enable you to capture market value and enhance your bottom line.";
    ViewBag.MetaKeywords = "nav_pricing , profitability, pricing strategies, revenue models, demand forecasting, market value, bottom line";


